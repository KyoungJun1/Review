<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings><!-- value 에 NULL 은 무조건 대문자로 -->
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<typeAliases>

		<typeAlias type="com.kkj.myapp.Member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kkj.myapp.Library.model.vo.Library" alias="Library"/>
		<typeAlias type="com.kkj.myapp.Board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kkj.myapp.Board.model.vo.PagingVO" alias="PagingVO"/>

	</typeAliases>


	<mappers>

		<mapper resource="/mappers/member-mapper.xml" />
		<mapper resource="/mappers/board-mapper.xml" />
		<mapper resource="/mappers/library-mapper.xml" />


	</mappers>


</configuration>
